/**jiehuo.wxss**/
page{
  background: white;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0;
  background-color: white;
  box-sizing: border-box;
  font-family: '微软雅黑'
} 
form{
  width: 100%;
}
textarea {
  width: 668rpx;
  height: 150rpx;
  font-size: 30rpx;
  line-height: 30rpx;
  margin: 0 auto;
  padding-top: 35rpx; 
}
.addImg {
  padding:0 19rpx;
  display: flex;
  flex-direction: row;
  text-align: left 
}
.picBox {
  display: inline-block;
  min-width: 1rpx;
  min-height:149rpx
}
.picBox image{
  width: 179rpx;
  height: 180rpx;
}
.icon-addImg {
  display: inline-block;
  width: 169rpx;
  height: 175rpx;
  margin-left: 10rpx
}
.icon-addImg image{
  width: 100%;
  height: 100%;
}
.radios {
  width: 100%;
  border-top: 2rpx solid #E3E3E3;
  text-align: center;
  display: block;
  margin-top: 26rpx;
}
.radios navigator {
  display: inline-block;
}
.radios text {
  display:inline-block;
  font-size:32rpx;
  line-height:37rpx;
  width:74rpx;
  height:74rpx;
  padding:19rpx;
  color:#fff;
  background-color:#D3D3D3;
  border-radius:50%;
  margin:36rpx 16rpx 0;
  font-weight:bold;

}
.radios text.rd-active {
  background-color: #F96768;
}
.btn-area button {
  width: 681rpx;
  font-size: 32rpx;
  line-height: 32rpx;
  padding: 29rpx 275rpx;
  background-color: #3DB6FB;
  color: white;
  margin-top: 213rpx;
  margin-bottom: 213rpx;
}