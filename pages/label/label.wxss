/**label.wxss**/
.container {
  background-color: #F0F0F0;
}
.in-bl {display: inline-block;}
.f-l {float: left;}
.f-r {float: right;}
.fs-28 {font-size: 28rpx;} 
.fs-30 {font-size: 30rpx;}
.ds-n{display: none;}
.title {
  width: 100%;
  padding: 28rpx 0 0;
}
.title text{
  color: #666;
  margin-left: 36rpx;
}
.title .confirm{
  color: #43C4FF;
  margin-right: 36rpx;
}
.box {
  width: 96%;
  height: 216rpx;
  background-color: #fff;
  padding:10rpx 2% 24rpx;
  margin-top: 55rpx;
}
.box text {
  color:#333;
  display:inline-block;
  line-height:30rpx;
  padding:20rpx 0;
  border:2rpx solid #3db6fb;
  border-radius:20px;
  margin:24rpx 28rpx 10rpx;
  width:181rpx;
  text-align:center;
}
.search {
  width: 726rpx;
  height: 57rpx;
  background-color: #fff;
  margin: 16rpx auto;
  border-radius: 10rpx;
  text-align: center;
  border:2rpx solid #e1e1e1;
  font-size: 26rpx;
}
.placeholder-style {
  text-align: center;
  line-height: 24rpx;
  background-image: url("data:image/png;base64,");
  background-repeat: no-repeat;
  background-size:30rpx 30rpx;
  background-position: 43%;
}
.placeholder-none {
  top: 0
}
.ph-Class{
  margin-left: 25rpx; 
}
.tab-box {
  width: 100%;
  height: 746rpx;
  margin-bottom: 11rpx;
  border-top: 2rpx solid #D3D3D3;
  background-color: #F8F8F8;
}
.tab{
  display: flex;
  flex-direction: column;
}
.tab text {
  padding: 57rpx 88rpx 57rpx 83rpx;
  font-size: 28rpx;
  color: #333;
}
.tab-active {
  border-left: #17B1F7 10rpx solid;
}
.tab-noactive {
  border-left: #F8F8F8 10rpx solid;
}
.tab-list{
  width: 432rpx;
  height: 746rpx;
  background-color: #fff;
}
.tab-list text {
  display: block;
  width: 432rpx;
  padding: 32rpx 0;
  text-align: center;
  font-size: 28rpx;
  color: #666;
  border-bottom: 2rpx solid #D3D3D3;
}
